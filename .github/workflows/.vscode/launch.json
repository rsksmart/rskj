    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickJavaProcess}"
        },

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debugger for Mainframe: INTERTESTâ„¢ FOR BATCH",
            "type": "intertest-batch",
            "request": "launch",
            "programName": [
                "PROGRAMNM"
            ],
            "protsym": [
                "PROTSYM"
            ],
            "interTestHost": "HOST",
            "interTestPort": 0,
            "interTestUserName": "USER",
            "interTestSecure": true,
            "convertedJCL": "CONV.JCL(RUNME)"
        }
    ]
}